<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<AnchorPane fx:id="loginAnchorPane" prefHeight="600.0" prefWidth="300.0" style="" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <ImageView fitHeight="180.0" fitWidth="230.0" layoutX="35.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="loginButton" onAction="#login" layoutX="110.0" layoutY="382.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Login" />
      <TextField fx:id="userTextField" layoutX="35.0" layoutY="294.0" prefHeight="25.0" prefWidth="230.0" promptText="Username" />
      <TextField fx:id="passwordTextField" layoutX="35.0" layoutY="331.0" prefHeight="25.0" prefWidth="230.0" promptText="Password" />
      <Button fx:id="regButton" layoutX="111.0" layoutY="418.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Register" />
      <TextField fx:id="ipTextField" layoutX="35.0" layoutY="258.0" prefHeight="25.0" prefWidth="230.0" promptText="Server IP" />
   </children>
</AnchorPane>
